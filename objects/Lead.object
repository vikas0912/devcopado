<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Annual_reveneu_field_is_greater_than_0</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Rating , &quot;Hot&quot;) ,
 OR( ISBLANK( AnnualRevenue )  , AnnualRevenue &lt;= 0

)

      )</errorConditionFormula>
        <errorMessage>Annual field should be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>lead_source_cannot_be_empty</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 ISPICKVAL( Status , &quot;Qualified&quot;)  , ISBLANK(Text( LeadSource) ) 
)</errorConditionFormula>
        <errorMessage>&quot;Lead Source must be filled out when Lead Status is set to Qualified.&quot;</errorMessage>
    </validationRules>
</CustomObject>
